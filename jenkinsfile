pipeline {
    agent any
    stages {
        stage("Checkout") {
            steps {
             git url: 'https://github.com/GeorgesNgandeu/jenkins-docker.git'
            }
        }
        stage("Compile stage") {
            steps {
             sh "./gradlew compileJava"
            }
        }
        stage("Test stage") {
            steps {
                echo 'Test stage when setup'
            }
        }
    }
}